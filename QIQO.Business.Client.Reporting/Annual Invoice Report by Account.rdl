          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="account_name">
          <DataField>account_name</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="account_desc">
          <DataField>account_desc</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
