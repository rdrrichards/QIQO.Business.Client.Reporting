      <rd:DataSourceID>6e1c0bd7-b694-43e2-9c3b-9bd9f31250cd</rd:DataSourceID>
    </DataSource>
  </DataSources>
  <DataSets>
    <DataSet Name="ReportMain">
      <Query>
        <DataSourceName>SourceMain</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@Account">
            <Value>=Parameters!Account.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@StartDate">
            <Value>=Parameters!StartDate.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@EndDate">
            <Value>=Parameters!EndDate.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT A.invoice_num, CAST(A.invoice_entry_date AS date) AS invoice_date, 
ROUND(A.invoice_value_sum, 0) AS total, D.account_name, 
MIN(CAST(C.order_item_ship_date AS date)) AS [first_ship_date], MAX(CAST(C.order_item_ship_date AS date)) AS [last_ship_date]
FROM invoice A JOIN invoice_status B
ON A.invoice_status_key = B.invoice_status_key
JOIN invoice_item C
ON A.invoice_key = C.invoice_key
JOIN account D
ON A.account_key = D.account_key
WHERE A.account_key = @Account
AND A.invoice_status_key = 4
AND A.invoice_entry_date BETWEEN @StartDate AND @EndDate

GROUP BY A.invoice_num, CAST(A.invoice_entry_date AS date), D.account_name, A.invoice_value_sum</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="invoice_num">
          <DataField>invoice_num</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="invoice_date">
          <DataField>invoice_date</DataField>
          <rd:TypeName>System.DateTime</rd:TypeName>
        </Field>
        <Field Name="total">
          <DataField>total</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
        <Field Name="account_name">
          <DataField>account_name</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="first_ship_date">
          <DataField>first_ship_date</DataField>
          <rd:TypeName>System.DateTime</rd:TypeName>
        </Field>
        <Field Name="last_ship_date">
          <DataField>last_ship_date</DataField>
          <rd:TypeName>System.DateTime</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="AccountList">
      <Query>
        <DataSourceName>SourceMain</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@Company">
            <Value>=Parameters!Company.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT account_key, account_code, account_name,
account_code + ' - ' + account_name AS account_desc
FROM account 
WHERE company_key = @Company
ORDER BY 3</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="account_key">
          <DataField>account_key</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="account_code">
